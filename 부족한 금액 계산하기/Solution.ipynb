   "source": [
    "def solution(price, money, count):\r\n",
    "    total = 0\r\n",
    "    for i in range(count+1):\r\n",
    "        total += (price*i)\r\n",
    "\r\n",
    "    return max(0, abs(money-total))\r\n",
    "\r\n",
    "# Code Test\r\n",
    "\r\n",
    "output = solution(3, 20, 4)\r\n",
    "\r\n",
    "print(\"price : 3 \\t money : 20 \\t count : 4 \\t result : \", output)\r\n"
